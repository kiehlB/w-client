{
  "name": "@w/react-utils",
  "version": "2.0.10",
  "description": "A set of utilities for react on client side",
  "keywords": [
    "react-utils"
  ],
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "pnpm build:fast --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "dependencies": {
    "@w/shared-utils": "workspace:*",
    "@w/react-rsc-utils": "workspace:*"
  },
  "devDependencies": {
    "clean-package": "2.2.0",
    "react": "^18.0.0"
  },
  "clean-package": "../../../clean-package.config.json"
}
