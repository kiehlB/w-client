{
  "name": "@w/apollo-client",
  "version": "2.0.0",
  "description": "",
  "keywords": [
    ""
  ],
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "pnpm build:fast --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "devDependencies": {
    "clean-package": "2.2.0"
  },
  "clean-package": "../../../clean-package.config.json",
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.9.9",
    "@apollo/experimental-nextjs-app-support": "^0.9.0"
  }
}
