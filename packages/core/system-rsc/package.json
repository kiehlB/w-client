{
  "name": "@w/system-rsc",
  "version": "2.0.11",
  "keywords": [
    "system-rsc"
  ],
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "pnpm build:fast --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "react": ">=18",
    "@w/theme": ">=2.1.0",
    "tailwind-variants": ">=0.1.13"
  },
  "devDependencies": {
    "react": "^18.0.0",
    "tailwind-variants": "^0.1.18",
    "@w/theme": "workspace:*",
    "@w/react-utils": "workspace:*",
    "clean-package": "2.2.0"
  },
  "dependencies": {
    "clsx": "^1.2.1"
  },
  "clean-package": "../../../clean-package.config.json",
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  }
}
