    "@typescript-eslint/parser": "^5.42.0",
    "chalk": "^4.1.2",
    "concurrently": "^7.6.0",
    "commitlint-plugin-function-rules": "^1.7.1",
    "intl-messageformat": "^10.1.0",
    "execa": "^5.1.1",
