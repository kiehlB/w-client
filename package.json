    "tsx": "^3.8.2",
    "@commitlint/cli": "^17.2.0",
    "@react-types/link": "^3.4.4",
    "@react-types/shared": "^3.19.0",
    "@swc-node/jest": "^1.5.2",
    "@swc/core": "^1.3.35",
    "@swc/jest": "^0.2.24",
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
